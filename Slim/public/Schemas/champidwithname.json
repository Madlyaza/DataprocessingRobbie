{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Draft 07 schema for champions",
    "description": "A schema for validating the Champidwithname database",
    "type": "array",
    "properties": {
        
        "ChampId": {
            "type": "integer",
            "minimum": 1,
            "maximum": 1000
        },

        "ChampName": {
            "type": "string",
            "minimum": 1,
            "maximum": 999999999
        }
    }
}